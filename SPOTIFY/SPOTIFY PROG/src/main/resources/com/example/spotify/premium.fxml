    <children>
      <ImageView fitHeight="799.0" fitWidth="1264.0" layoutX="26.0" layoutY="57.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/premium.jpg" />
         </image>
      </ImageView>
        <Button fx:id="primero" layoutX="59.0" layoutY="660.0" mnemonicParsing="false" onAction="#primero" prefHeight="62.0" prefWidth="243.0" style="-fx-background-color: transparent;" />
